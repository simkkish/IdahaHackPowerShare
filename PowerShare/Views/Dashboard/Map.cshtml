
@{
    ViewData["Title"] = "Map";
}

<h1>Map</h1>
<div id="map" style="height:100%; width:70%; position: absolute;">

</div>
<script type="text/javascript">

    function initialize() {
        var latlng = new google.maps.LatLng(42.866550, -112.443530);
        var myOptions = {
            zoom: 8,
            center: latlng
        };
        var map = new google.maps.Map(document.getElementById("map"),
            myOptions);
    }
    google.maps.event.addDomListener(window, "load", initialize);

</script>



